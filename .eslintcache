\",\r\n    password: \"\",\r\n    error: \"\",\r\n    loading:false,\r\n    redirectToReferrer:false\r\n  })\r\n\r\n  const {email,password,error,redirectToReferrer,loading}=values;\r\n  const {user}= isAuthenticated();\r\n  \r\n  const handleChange = name => event =>{\r\n     setValues({...values, error: false, [name]: event.target.value})\r\n  }\r\n\r\n  const clickSubmit = (event)=>{\r\n \r\n    event.preventDefault();\r\n    \r\n    setValues({...values,error:false,loading:true})\r\n    signin({email,password}).then(data =>{\r\n      console.log(data)\r\n      if (data.message) {\r\n        setValues({...values,error:data.error,loading:false})\r\n        setUser(false);\r\n        swal(data.message)\r\n      }\r\n      else{\r\n       \r\n        authenticate( data,()=>{setValues({...values,redirectToReferrer:true})})\r\n        \r\n      }\r\n    })\r\n  }\r\n\r\n  const redirectUser = ()=>{\r\n    if (redirectToReferrer) {\r\n      if (user) {\r\n        swal(\"Bienvenido¡\");\r\n        setUser(true);\r\n        \r\n        return <Redirect to=\"/dashboard\"/>\r\n      }\r\n      \r\n    }\r\n  }\r\n\r\n  const showLoading = ()=>(\r\n    loading && (\r\n      <div className=\"alert alert-info\"> \r\n        <h2>Cargando</h2>\r\n      </div>\r\n    )\r\n  )\r\n  \r\n    return(\r\n<div className=\"container\" id=\"registro\">\r\n\r\n<form>\r\n\r\n<div style={{textAlign:\"center\"}}><label style={{fontFamily:\"sans-serif\",fontWeight:\"bold\" }}>Iniciar Sesion</label></div>\r\n  \r\n  <div className=\"form-group content-center\">\r\n    <label>Correo Electronico</label>\r\n    <input onChange={handleChange('email')} type=\"email\" value={email} className=\"form-control\" id=\"exampleInputEmail1\"  aria-describedby=\"emailHelp\" placeholder=\"Introduce tu correo electronico\"/>\r\n  </div>\r\n  <div className=\"form-group\">\r\n    <label>Contraseña</label>\r\n    <input onChange={handleChange('password')} type=\"password\" value={password} className=\"form-cont